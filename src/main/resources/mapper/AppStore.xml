<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="AppStore">
    <resultMap type="com.xiaomi.bean.AppStore" id="AppStoreResult">
        <!--主键用id，其他的用result-->
        <!--columu表示数据库里边的  property表示bean里边与数据库对应的-->
        <id column="StoreId" jdbcType="INTEGER" property="StoreId"/>
        <result column="StoreName" jdbcType="VARCHAR" property="StoreName"></result>
    </resultMap>




</mapper>